{{#if coches}}
<link rel="stylesheet" href="/css/admin.css">
<div class="row">
  <div class="col-lg-12">
    <rd-widget>
      <rd-widget-header icon="fa-tasks" title="Servers">
        <a href="#">Coches Disponibles</a>
      </rd-widget-header>
      <rd-widget-body classes="medium no-padding">
        <div class="table-responsive">
          <table class="table">
            <tbody>
              {{#each coches}}
              <tr><td>{{marca}}</td><td>{{modelo}}</td><td>{{precio}}</td><td>{{coche.potencia}}cv</td><td><span class="text-success"><i class="fa fa-check"></i></span></td></tr> {{/each}}
            </tbody>
          </table>
        </div>      
      </rd-widget-body>
    </rd-widget>
  </div>
</div>
{{/if }}